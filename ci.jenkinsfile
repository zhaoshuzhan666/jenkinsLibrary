#!groovy
@Library('jenkinsLibrary'@master) _
def build = new org.devops.build()
pipeline{
    agent { node { label "master"} }

    stages{
        stage("build"){
            steps{
                script{
                    build.Build(buildType,buildShell)
                   // mvnHome = tool "m2"
                   // sh "${mvnHome}/bin/mvn -v"
                }
            }
        }
    }
}